{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "productMainCategory": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "DRINKS"
      ],
      "required": true,
      "private": true,
      "default": "DRINKS"
    },
    "alcoholicDrinkType": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "BEER",
        "GRAPE_WINE",
        "CHAMPAGNE",
        "SPIRIT",
        "OTHER"
      ],
      "required": false
    },
    "nonAlcoholicDrinkType": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "COFFEE_OR_COFFEE_POWDER",
        "COFFEE_CAPSULE_OR_POD",
        "TEA_OR_MEDICINAL_TEA",
        "MINERAL_WATER",
        "SODA_POP",
        "ICE_TEA",
        "ENERGY_DRINK",
        "ALCOHOL_FREE_BEER",
        "COCKTAIL_SYRUP",
        "SYRUP",
        "FRUIT_OR_VEGETABLE_JUICE"
      ],
      "required": false
    },
    "brand": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "ean": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true,
      "maxLength": 13,
      "minLength": 13,
      "regex": "^[0-9]{13}$"
    },
    "vatRate": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "TWENTY_SEVEN",
        "EIGHTEEN"
      ],
      "required": true
    },
    "packaging": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "BOTTLE",
        "BOX",
        "PLASTIC_BOTTLE",
        "BAG_IN_BOX",
        "BARREL",
        "JUG",
        "KEG"
      ],
      "required": true
    },
    "netAmount": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "unit": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "MILLILITERS",
        "CENTILITERS",
        "DECILITERS",
        "LITERS",
        "GRAMS",
        "DECAGRAMS",
        "KILOGRAMS",
        "PIECES"
      ],
      "required": true
    },
    "equalizedAmount": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "required": true
    },
    "grossWeightInKilograms": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "decimal",
      "min": 0,
      "required": true
    },
    "packagingFreeProduct": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "required": true
    },
    "customerSegment": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "properties.customer-segment",
      "required": true
    },
    "alcoholicDrinkProperties": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "properties.alcoholic-drink-props"
    },
    "knCode": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true,
      "maxLength": 8,
      "minLength": 8,
      "regex": "^[0-9]{8}$"
    },
    "allergenic": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "properties.allergenic",
      "required": true
    },
    "productImages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-image.product-image",
      "inversedBy": "products"
    },
    "placeOfOrigin": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "hoReCa": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "required": true
    },
    "shortDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "maxLength": 320,
      "minLength": 1
    },
    "detailedDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true,
      "minLength": 1
    },
    "merchantId": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "regex": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "required": true,
      "maxLength": 36,
      "minLength": 36
    },
    "nonAlcoholicDrinkProperties": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "properties.non-alcoholic-drink-properties"
    },
    "optionalProperties": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "properties.optional-product-props"
    },
    "secondaryProductSheet": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "required": true
    },
    "videoLink": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "rejectionReason": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "text"
    },
    "consentProps": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "properties.consent-props"
    }
  }
}
